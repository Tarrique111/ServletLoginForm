<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>registrationForm</display-name>
	<welcome-file-list>
		<welcome-file>signin.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>registerController</servlet-name>
		<servlet-class>com.bridgelabz.Controller.registerController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registerController</servlet-name>
		<url-pattern>/registerController</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.bridgelabz.Controller.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.bridgelabz.Controller.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>register</servlet-name>
		<jsp-file>/register.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>sigin</servlet-name>
		<jsp-file>/sigin.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>sigin</servlet-name>
		<url-pattern>/sigin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>profile</servlet-name>
		<jsp-file>/profile.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
</web-app>